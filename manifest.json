{
    "name": "CRS-RUPP Integration",
    "version": "0.1.1.0",
    "manifest_version": 3,
    "description": "Runs (old) RUPP on CRS Preenlistment",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "CRS-RUPP Integration",
        "default_popup": "popup.html"
    }
}